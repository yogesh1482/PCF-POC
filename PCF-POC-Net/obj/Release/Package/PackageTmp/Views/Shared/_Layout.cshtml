<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>MUST HAVE - @ViewData["Title"]</title>

    <environment names="Development">
        <link href="~/Content/Site.css" rel="stylesheet" />
        <link href="~/Content/bootstrap.css" rel="stylesheet" />
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/css/bootstrap.min.css"
              asp-fallback-href="~/Content/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/Content/site.min.css" asp-append-version="true" />
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-T8Gy5hrqNKT+hzMclPo118YTQO6cYprQmhrYwIiQ/3axmI1hQomh7Ud2hPOy8SP1" crossorigin="anonymous"
              asp-fallback-href="~/lib/font-awesome/css/font-awesome.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
    </environment>
    @*@Html.ApplicationInsightsJavaScript(TelemetryConfiguration)*@
</head>
<body>
    <nav class="header navbar navbar-default" style="margin-bottom:0px;top:0px;">
        <div class="container-fluid">
            @*style='padding: 0px 0px 0px 0px;'*@
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header col-md-12" style="margin:0px 0px 0px 0px;height:75.40px">
                @Html.Raw(ViewData["Header1"])
            </div>
        </div><!-- /.container-fluid -->
    </nav>
    <div class="container-fluid body-content" style="margin:0px 0px 0px 0px;padding:0px 0px 0px 0px;">
        @RenderBody()
    </div>

    <environment names="Development">
        <script src="~/Scripts/bootstrap.js"></script>
        <script src="~/Scripts/jquery-1.10.2.js"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-1.10.0.min.js"
                asp-fallback-src="~/Scripts/jquery-1.10.2.min.js"
                asp-fallback-test="window.jQuery">
        </script>

        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/bootstrap.min.js"
                asp-fallback-src="~/Scripts/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
        </script>
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>
